<script type="text/javascript">
            // push settings with "false" save to local
            //initApp.pushSettings("nav-function-minify layout-composed", false);


	//每隔1小时重置页面
	//setInterval("window.location.reload()",1000 * 10);

	function getQueryVariable(variable)
	{
		   var query = window.location.search.substring(1);
		   var vars = query.split("&");
		   for (var i=0;i<vars.length;i++) {
				   var pair = vars[i].split("=");
				   if(pair[0] == variable){return pair[1];}
		   }
		   return(false);
	}

	var sn = (getQueryVariable("sn") === false ? "e8491d763d078b41" : getQueryVariable("sn"));
	//所有iframe后面加上sn参数
	var iframes = $("iframe")
	for (i in iframes) {
	  iframes[i].src = iframes[i].src +"?sn=" +sn;
	}

	//停止carousel循环播放
	$('#carouselExampleControls').carousel({
		pause: true,
		interval: false
	});
	//当滚动html时，刷新一下最新的html
	$('#carouselExampleControls').on('slide.bs.carousel', function (evt) {
			 //$.ajax({
			 // url: 'https://commonservices.eorionsolution.com/eim/eink/img/logo.png?v='+new Date().getTime(),
			 // type: 'GET',
			 // complete: function(response) {
			 //  if(response.status == 200) {
			 //  } else {
			//	alert('网络异常');
			 //  }
			 // }
			 //});
		  //console.debug("slide transition started")
		  //console.debug('current slide = ', $(this).find('.active').index())
		  //console.debug('next slide = ', $(evt.relatedTarget).index())
		  //$(this).find('.active')[0].children[0].contentWindow.location.reload(true);
		  iframes[$(evt.relatedTarget).index()].contentWindow.location.reload(true);
		  $('#pageTitle').text(iframes[$(evt.relatedTarget).index()].name);
	});
	
   $('a[id^="link"]').on('click', function (e) {
		
		var id = $(this).attr("id").replace("link","")
		iframes[id].contentWindow.location.reload(true);
		$('#pageTitle').text(iframes[id].name);	
		$('#carouselExampleControls').carousel(parseInt(id))				
    });
	
	
	function clickNext() {
		$('#nextButton').click();
	}
	setInterval(clickNext, 30*1000);	

</script> 
  
  

